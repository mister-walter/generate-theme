<meta charset="{{ site.charset }}" />
<title>
  {% if wp_title %} {{ site.name }} | {{ wp_title }} {% else %}
  {{ site.name }} {% endif %}
</title>
<meta name="description" content="{{ site.description }}" />
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<link
  rel="icon"
  type="image/png"
  href="{{ site.theme.link }}/images/favicon.png"
  hreflang="{{ site.language }}"
/>
<meta name="description" content="{{ site.description }}" />
<meta name="keywords" content="{{ keywords }}" />
<meta itemprop="name" content="{{ site.title }}" />
<meta itemprop="description" content="{{ site.description }}" />
<meta itemprop="image" content="" />
<!-- Google Fonts -->
<link
  href="https://fonts.googleapis.com/css?family=Lato:400,400i,700,700i"
  rel="stylesheet"
/>
<link
  href="https://fonts.googleapis.com/css?family=IBM+Plex+Sans:400,400i,600,600i,700,700i"
  rel="stylesheet"
/>

<!-- Stylesheet -->
<link
  rel="stylesheet"
  href="{{ site.theme.link }}/style.css"
  type="text/css"
  media="screen"
/>
 <!-- Google Search Console verification tag -->
<meta name="google-site-verification" content="LZVUpVWUA2xcHLNj1F9gDiShijhbZj8KUp-V_U69RK0" />

{{ function('wp_head') }}
